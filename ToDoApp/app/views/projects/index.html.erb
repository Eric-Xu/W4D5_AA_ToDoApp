<h1>Welcome to the Projects Page</h1>
<table>
	<tr>
		<th>Title</th>
		<th>Description</th>
		<th>Team ID</th>
	</tr>
	<% @projects.each do |project| %>
		<tr>
			<td><%= project.title %></td>
			<td><%= project.description %></td>
			<td><%= project.team.name %></td>
			<td><%= link_to "Edit", edit_project_path(project.id) %></td>
		</tr>
	<% end %>
</table>

<%= link_to "Add a new project", new_project_path %>