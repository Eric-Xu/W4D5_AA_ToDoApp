<h1>Team: <%= @team.name %></h1>
<h3>Projects:</h3>
<ol>
	<% @team.projects.each do |project| %>
		<li><%= link_to project.title, project_path(project.id) %></li>
	<% end %>
</ol>
<%= link_to "Make a new project", new_team_project_path(@team) %>

<h3>Members:</h3>
<ol>
	<% @team.users.each do |user| %>
		<li><%= link_to user.full_name, user_path(user.id) %></li>
	<% end %>
</ol>
<%= link_to "Enter a new member", new_user_path %>
